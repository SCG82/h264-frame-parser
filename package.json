{
  "name": "h264-frame-parser",
  "version": "0.0.0",
  "keywords": [
    "h264",
    "bitstream",
    "parser",
    "metadata",
    "sync"
  ],
  "description": "H.264 bitstream parsing + metadata sync helpers",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "docs": "typedoc lib/**.ts lib/*/*.ts --out docs",
    "prepare": "rm -rf dist; tsc",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "files": [
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medooze/h264-frame-parser.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medooze/h264-frame-parser/issues"
  },
  "homepage": "https://github.com/medooze/h264-frame-parser#readme",
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "transform": {
      "\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/lib/sync"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 60,
        "lines": 85,
        "statements": 85
      }
    },
    "collectCoverageFrom": [
      "lib/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  }
}
